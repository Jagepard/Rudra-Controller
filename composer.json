{
    "name": "rudra/controller",
    "description": "Rudra framework",
    "type": "library",
    "keywords": [
        "rudra",
        "controller",
        "mvc-pattern",
        "mvc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jagepard",
            "email": "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "jagepard@yandex.ru",
        "issues": "https://github.com/Jagepard/Rudra-Controller/issues",    
        "source": "https://github.com/Jagepard/Rudra-Controller"    
    },
    "require": {
        "php": ">=8.3",
        "rudra/docs": "self.version"
    },
    "autoload": {
        "psr-4" : {
            "Rudra\\Controller\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rudra\\Controller\\Tests\\" : "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
